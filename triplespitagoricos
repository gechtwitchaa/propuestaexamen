#include <stdio.h>

int main() {
    // Definir los triples pitagóricos específicos
    int pitagoric_triplets[4][3] = {
            {3, 4, 5},
            {6, 8, 10},
            {5, 12, 13},
            {8, 15, 17}
    };

    // Iterar a través de la lista de triples pitagóricos y imprimirlos
    for (int i = 0; i < 4; i++) {
        printf("- %d - %d - %d\n", pitagoric_triplets[i][0], pitagoric_triplets[i][1], pitagoric_triplets[i][2]);
    }

    return 0;
}
